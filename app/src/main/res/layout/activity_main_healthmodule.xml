<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bubles1"
    tools:context=".MainActivityHealthmodule">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/texthealth"
                android:layout_width="263dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="100dp"
                android:layout_marginTop="50dp"
                android:layout_marginRight="100dp"
                android:text="health Module"
                android:textAllCaps="true"
                android:textColor="#000000"
                android:textSize="30sp"
                android:textStyle="bold" />

            <ImageButton
                android:onClick="btnhealthmodule1"
                android:layout_width="306dp"
                android:layout_height="89dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="70dp"
                android:background="#0000"
                android:scaleType="fitStart"
                app:srcCompat="@drawable/custombtnmodule1" />

            <ImageButton
                android:onClick="btnhealthmodule2"
                android:layout_width="306dp"
                android:layout_height="89dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="70dp"
                android:background="#0000"
                android:scaleType="fitStart"
                app:srcCompat="@drawable/custombtnmodule2" />

            <ImageButton
                android:onClick="btnhealthmodule3"
                android:layout_width="306dp"
                android:layout_height="89dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="70dp"
                android:background="#0000"
                android:scaleType="fitStart"
                app:srcCompat="@drawable/custombtnmodule3" />

            <ImageButton
                android:onClick="btnhealthmodule4"
                android:layout_width="306dp"
                android:layout_height="89dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="70dp"
                android:background="#0000"
                android:scaleType="fitStart"
                app:srcCompat="@drawable/custombtnmodule4" />

            <ImageButton
                android:onClick="btnhealthmodule5"
                android:layout_width="306dp"
                android:layout_height="89dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="70dp"
                android:background="#0000"
                android:scaleType="fitStart"
                app:srcCompat="@drawable/custombtnmodule5" />

            <ImageButton
                android:onClick="btnquizhealth"
                android:layout_width="306dp"
                android:layout_height="89dp"
                android:layout_marginLeft="70dp"
                android:layout_marginTop="20dp"
                android:layout_marginRight="70dp"
                android:background="#0000"
                android:scaleType="fitStart"
                app:srcCompat="@drawable/custombtnquizhealth" />


        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>